{
  "class": "org.unipop.elastic.ElasticSourceProvider",
  "clusterName": "unipop",
  "addresses": ["http://localhost:9200"],
  "vertices": [
    {
      "type": "document",
      "index": "vertex",
      "es_type": "person",
      "id": "@_id",
      "label": "person",
      "dynamicProperties": {
        "excludeFields": ["knownBy", "weight", "edgeId"]
      },
      "edges": [{
        "type": "document",
        "direction": "IN",
        "id": "@edgeId",
        "label": "knows",
        "properties": {
          "weight": "@weight",
          "name": "@edgeName"
        },
        "vertex": {
          "type": "ref",
          "id": "@knownBy",
          "label": "person",
          "name": "@vertex_name"
        }
      }]
    },
    {
      "type": "document",
      "index": "vertex",
      "id": "@_id",
      "label": {
        "field": "_type",
        "exclude": ["person"]
      },
      "properties": {},
      "dynamicProperties": true
    }
  ],
  "edges": [
    {
      "type": "document",
      "index": "edge",
      "id": "@_id",
      "label": {
        "field": "_type",
        "exclude": ["knows"]
      },
      "properties": {},
      "dynamicProperties": {
        "excludeFields": ["outId", "inId", "outLabel", "inLabel"]
      },
      "outVertex": {
        "type": "ref",
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "type": "ref",
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    }
  ]
}