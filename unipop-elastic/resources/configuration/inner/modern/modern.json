{
  "class": "org.unipop.elastic.ElasticSourceProvider",
  "clusterName": "unipop",
  "addresses": ["http://localhost:9200"],
  "vertices": [
    {
      "index": "vertex",
      "type": "person",
      "id": "@_id",
      "label": "person",
      "dynamicProperties": {
        "excludeFields": ["edgeId", "knownBy", "edgeWeight", "edgeName", "_type"]
      },
      "edges": [{
        "id": "@edgeId",
        "label": "knows",
        "direction": "IN",
        "properties": {
          "weight": {
            "field": "edgeweight",
            "type": "DOUBLE",
            "nullable": true
          },
          "name": "@edgeName"
        },
        "vertex": {
          "ref": true,
          "id": "@knownBy",
          "label": "person"
        }
      }]
    },
    {
      "index": "vertex",
      "id": "@_id",
      "label": {
        "field": "_type",
        "exclude": ["person"]
      },
      "properties": {},
      "dynamicProperties": true
    }
  ],
  "edges": [
    {
      "index": "edge",
      "id": "@_id",
      "label": {
        "field": "_type",
        "exclude": ["knows"]
      },
      "properties": {
        "weight": {
          "field": "weight",
          "type": "DOUBLE",
          "nullable": true
        }
      },
      "dynamicProperties": {
        "excludeFields": ["outId", "inId", "outLabel", "inLabel"]
      },
      "outVertex": {
        "ref": true,
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "ref": true,
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    }
  ]
}