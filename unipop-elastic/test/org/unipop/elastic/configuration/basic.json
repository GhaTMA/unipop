{
  "class": "ElasticSourceProvider",
  "client": "TRANSPORT",
  "clusterName": "unipop",
  "address": "127.0.0.1:9300",
  "vertices": [
    {
      "type": "document",
      "index": "vertex",
      "id": "@id",
      "label": "@type",
      "properties": {},
      "dynamicProperties": true
    }
  ],
  "edges": [
    {
      "type": "document",
      "index": "edge",
      "id": "@id",
      "label": "@type",
      "properties": {},
      "dynamicProperties": {
        "excluded": ["outId", "inId", "ourLabel", "inLabel"]
      },
      "outVertex": {
        "type": "ref",
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "type": "ref",
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    }
  ]
}