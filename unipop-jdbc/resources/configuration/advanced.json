{
  "class": "org.unipop.jdbc.JdbcSourceProvider",
  "address": "jdbc:sqlite:test.sqlite",
  "sqlDialect": "SQLITE",
  "vertices": [
    {
      "table": "vertices_modern",
      "id": "@ID",
      "label": "@LABEL",
      "properties": {
        "name": "@name",
        "age": "@age",
        "lang": "@lang"
      },
      "dynamicProperties": false
    },
    {
      "table": "vertices_crew",
      "id": "@ID",
      "label": "@LABEL",
      "properties": {
        "name": "@name",
        "location": "@location"
      },
      "dynamicProperties": false
    }
  ],
  "edges": [
    {
      "table": "edges_modern",
      "id": "@ID",
      "label": "@LABEL",
      "properties": {
        "weight": "@weight"
      },
      "dynamicProperties": false,
      "outVertex": {
        "ref": "true",
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "ref": "true",
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    },
    {
      "table": "edges_crew",
      "id": "@ID",
      "label": "@LABEL",
      "properties": {
        "since": "@since",
        "skill": "@skill"
      },
      "dynamicProperties": false,
      "outVertex": {
        "ref": "true",
        "id": "@outId",
        "label": "@outLabel",
        "properties": {}
      },
      "inVertex": {
        "ref": "true",
        "id": "@inId",
        "label": "@inLabel",
        "properties": {}
      }
    }

  ]
}