{
  "class": "org.unipop.jdbc.JdbcSourceProvider",
  "driver": "org.h2.Driver",
  "address": "jdbc:h2:mem:gremlin;",
  "sqlDialect": "H2",
  "vertices": [
    {
      "table": "PERSON_CREW",
      "id": "@ID",
      "label": "person",
      "properties": {
        "name": "@NAME",
        "location": "@LOCATION"
      }
    },
    {
      "table": "SOFTWARE_CREW",
      "id": "@ID",
      "label": "software",
      "properties": {
        "name": "@NAME"
      }
    }
  ],
  "edges": [
    {
      "table": "DEVELOPS_CREW",
      "id": "@ID",
      "label": "develops",
      "properties": {
        "since": "@SINCE"
      },
      "dynamicProperties": false,
      "outVertex": {
        "ref": "true",
        "id": "@OUTID",
        "label": "@OUTLABEL",
        "properties": {}
      },
      "inVertex": {
        "ref": "true",
        "id": "@INID",
        "label": "@INLABEL",
        "properties": {}
      }
    },
    {
      "table": "USES_CREW",
      "id": "@ID",
      "label": "uses",
      "properties": {
        "skill": "@SKILL"
      },
      "dynamicProperties": false,
      "outVertex": {
        "ref": "true",
        "id": "@OUTID",
        "label": "@OUTLABEL",
        "properties": {}
      },
      "inVertex": {
        "ref": "true",
        "id": "@INID",
        "label": "@INLABEL",
        "properties": {}
      }
    }
  ]
}