{
  "class": "org.unipop.jdbc.JdbcSourceProvider",
  "driver": "org.postgresql.Driver",
  "address": "jdbc:postgresql://localhost:5432/postgres",
  "sqlDialect": "POSTGRES",
  "vertices": [
    {
      "table": "vertex_inner",
      "id": "@ID",
      "label": {
        "field": "LABEL",
        "include": [
          "person"
        ]
      },
      "properties":{
        "name": "@NAME",
        "age": "@AGE"
      },
      "dynamicProperties": false,
      "edges": [
        {
          "id": "@EDGEID",
          "label": "knows",
          "direction": "OUT",
          "properties": {
            "weight": "@EDGEWEIGHT"
          },
          "dynamicProperties":false,
          "vertex": {
            "ref": true,
            "id": {
              "field":"KNOWNBY",
              "nullable":false
            },
            "label": "person",
            "dynamicProperties":false
          }
        }
      ]
    },
    {
      "table": "vertex_inner",
      "id": "@ID",
      "label": {
        "field": "LABEL",
        "exclude": [
          "person"
        ]
      },
      "properties": {
        "name": "@NAME",
        "lang": "@LANG"
      },
      "dynamicProperties": false
    }
  ],
  "edges": [
    {
      "table": "edges",
      "id": "@ID",
      "label": {
        "field": "LABEL",
        "exclude": [
          "knows"
        ]
      },
      "properties": {
        "weight": "@WEIGHT"
      },
      "dynamicProperties": {
        "excludeProperties":[
          "name", "age"
        ],
        "excludeFields": [
          "OUTID",
          "INID",
          "OUTLABEL",
          "INLABEL"
        ]
      },
      "outVertex": {
        "ref": true,
        "id": "@OUTID",
        "label": "@OUTLABEL",
        "properties":{
        }
      },
      "inVertex": {
        "ref": true,
        "id": "@INID",
        "label": "@INLABEL",
        "properties":{
        }
      }
    }
  ]
}