{
  "class": "org.unipop.jdbc.JdbcSourceProvider",
  "driver": "org.postgresql.Driver",
  "address": ["jdbc:postgresql://localhost:5432/postgres"],
  "sqlDialect": "POSTGRES",
  "optimized": true,
  "vertices": [
    {
      "table": "ANIMAL_MODERN",
      "id": "@ID",
      "label": "animal",
      "properties": {
        "name": "@NAME",
        "age": {"field": "AGE", "type":"INT"}
      }
    },
    {
      "table": "vertex_inner",
      "id": "@ID",
      "label": {
        "field": "LABEL",
        "include": [
          "person"
        ]
      },
      "properties":{
        "name": "@NAME",
        "age": {"field": "AGE", "type": "INT"}
      },
      "dynamicProperties": false,
      "edges": [
        {
          "id": "@EDGEID",
          "label": "created",
          "direction": "OUT",
          "properties": {
            "weight": {"field": "EDGEWEIGHT", "type": "DOUBLE"}
          },
          "vertex": {
            "ref": false,
            "id": {
              "field":"CREATEDBY",
              "nullable":false
            },
            "label": "software",
            "properties":{
              "name": "@EDGENAME",
              "lang": "@EDGELANG"
            },
            "dynamicProperties":false
          }
        }
      ]
    }
  ],
  "edges": [
    {
      "table": "edges",
      "id": "@ID",
      "label": {
        "field": "LABEL",
        "exclude": [
          "created"
        ]
      },
      "properties": {
        "weight": {"field":"WEIGHT", "type": "DOUBLE"}
      },
      "dynamicProperties": {
        "excludeProperties":[
          "name", "lang", "age"
        ],
        "excludeFields": [
          "OUTID",
          "INID",
          "OUTLABEL",
          "INLABEL"
        ]
      },
      "outVertex": {
        "ref": true,
        "id": "@OUTID",
        "label": "@OUTLABEL",
        "properties":{
        }
      },
      "inVertex": {
        "ref": true,
        "id": "@INID",
        "label": "@INLABEL",
        "properties":{
        }
      }
    }
  ]
}