{
  "class": "org.unipop.jdbc.JdbcSourceProvider",
  "driver": "org.postgresql.Driver",
  "address": ["jdbc:postgresql://localhost:5432/postgres"],
  "sqlDialect": "POSTGRES",
  "optimized": true,
  "vertices": [
    {
      "table": "PERSON_MODERN",
      "id": "@id",
      "label": "person",
      "properties": {
        "name": "@name",
        "age": {"field":"age", "type": "INT"}
      }
    },
    {
      "table": "ANIMAL_MODERN",
      "id": "@id",
      "label": "animal",
      "properties": {
        "name": "@name",
        "age": "@age"
      }
    },
    {
      "table": "SOFTWARE_MODERN",
      "id": "@id",
      "label": "software",
      "properties": {
        "name": "@name",
        "lang": "@lang"
      }
    }
  ],
  "edges": [
    {
      "table": "MODERN_EDGES",
      "id": "@id",
      "label": "@label",
      "properties": {
        "weight": {"field": "weight", "type":"DOUBLE"},
        "year": "@year"
      },
      "dynamicProperties": false,
      "outVertex": {
        "ref": "true",
        "id": "@outid",
        "label": "@outlabel",
        "properties": {}
      },
      "inVertex": {
        "ref": "true",
        "id": "@inid",
        "label": "@inlabel",
        "properties": {}
      }
    }
  ]
}